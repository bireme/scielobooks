<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<p metal:use-macro="main.macros['master']">
    
  <div metal:fill-slot="custom_top" class="userbox">
    <span tal:condition="user" >
      <span tal:replace="user.username">Loro Jos√©</span> <a href="/logout">logout</a>
    </span>
    <span tal:condition="not user">
      <a href="/logout">login</a>
    </span>
  </div>

  <div class="middle" metal:fill-slot="middle">
    <table>
        <tr>
            <th>order</th>
            <th>title</th>
        </tr>
        <tr tal:repeat="part documents">
            <td tal:content="part.order" />
            <td tal:content="part.title" />
            <td><a class="PDF" href="#" tal:attributes="href part.pdf_url"><span>PDF</span></a></td>
            <td><a tal:attributes="href part.edit_url" 
                   i18n:translate="">edit</a>
            </td>
        </tr>
    </table>
    <a href="${request.url}/new">Add</a>
  </div>
</p>